#### BEGIN
[
    {
        "id": "1",
        "response_code": 200
    },
    {
        "id": "2",
        "response_code": 201
    }
]

#### BEGIN
// All params are strings
id = 1
[
    {
        "MyFirstParam": "MyFirstParamValue"
    }
]
#### OUTPUT
{
    "message": "Email response success",
    "code": 170
}

#### BEGIN
// All params are strings
id = 2
[
    {
        "MyFirstParam": "MyFirstParamValue"
    },
    {
        "MySecondParam": "MySecondParamValue"
    }
]
#### OUTPUT
{
    "message": "Email response success",
    "code": 112
}